{
  "name": "octokit-plugin-rename-branch",
  "version": "0.0.0-development",
  "description": "Octokit plugin to rename a branch in a github repository, its settings, and its code.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc report --reporter=html && open coverage/index.html",
    "lint": "prettier --check '{src,test}/**/*.{ts,md}' *.md package.json tsconfig.json",
    "lint:fix": "prettier --write '{src,test}/**/*.{ts,md}' *.md package.json tsconfig.json",
    "pretest": "npm run -s lint",
    "test": "jest --coverage"
  },
  "keywords": [
    "octokit",
    "plugin",
    "github",
    "branch"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "devDependencies": {
    "@octokit/core": "^2.4.1",
    "@octokit/plugin-paginate-rest": "^2.0.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "repository": "https://github.com/gr2m/octokit-plugin-rename-branch",
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
